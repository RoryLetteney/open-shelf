<!DOCTYPE html>
<html lang="en">
<%- include ../layout/head.ejs %>
<body>
  <%- include ../layout/header.ejs %>
  <main>
    <h2><span class="total-saved"><%= totalSaved %></span> Saved Book(s)</h2>
    <% results.forEach(book => { %>
      <section class="book">
        <%- include books/detail.ejs %>
        <a class="show-details" href="book/<%= book.id %>">View Details</a>
      </section>
    <% }); %>
  </main>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
</body>
</html>